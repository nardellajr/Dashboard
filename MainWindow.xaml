<Window x:Class="Dashboard.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:views="clr-namespace:Dashboard.Views"
        mc:Ignorable="d"
        Title="Mike's Dashboard" Height="768" Width="1024">
    
    <Grid>
        <TabControl>
            <TabItem>
                <TabItem.Header>
                    <StackPanel Orientation="Horizontal">
                        <Image Source="Images/Icon.png" Width="16" Height="16" Margin="0,0,5,0"/>
                        <TextBlock Text="Tab 1"/>
                    </StackPanel>
                </TabItem.Header>
                <views:HousingMarketFactorsView x:Name="HousingMarketFactorsView" />
            </TabItem>
        </TabControl>
    </Grid>
</Window>
